{
  "type": "influxdb",
  "groupGauges": "true",
  "groupCounters": "true",
  "metricTemplates": {
    "io": {
      "pattern": "com.github.mcfongtw.io.(?<benchmark>[a-zA-Z0-9_]*).(?<metric>[a-zA-Z0-9_]*)$",
      "tagKeys": ["benchmark", "metric"]
    }
  },
  "durationUnit": "nanoseconds",
  "rateUnit": "milliseconds",
  "sender" : {
    "type": "http",
    "host": "localhost",
    "port": "8086",
    "database": "demo",
    "jersey": {
      "gzipEnabled": "true"
    }
  }
}